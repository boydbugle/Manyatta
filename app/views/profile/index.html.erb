    <div class="profile">
        <%= image_tag("go.png", alt:"profile picture")%>
    </div>
    <!-- Nav tabs -->
    <div class="row">
    <ul class="nav nav-tabs nav-justified" role="tablist">
        <li role="presentation">
            <a href="#bookings" aria-controls="bookings" role="tab" data-toggle="tab">Bookings</a>
        </li>
        <!-- show the house tab if the current user owns any houses -->

        <%if current_user.houses.any?%>
        <li role="presentation">
            <a href="#houses" aria-controls="houses" role="tab" data-toggle="tab">Houses</a>
        </li>
        <%end%>
        <li role="presentation" class="active">
            <a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">Account settings</a>
        </li>
    </ul>

    <!-- Tab panes code -->
    <div class="tab-content">
        <div class="tab-pane fade in active" role="tabpanel" id="bookings">
            <p>...</p>
        </div>
        <!-- show the house tab if the current user owns any houses -->
        <div class="tab-pane fade" role="tabpanel" id="houses">
            <%current_user.houses.each do |house|%>
            <%= image_tag%>
            <p>
                <strong>Name:</strong>
                <%=house.name%>
            </p>
            <p>
                <strong>Location:</strong>
                <%=house.street%>
            </p>
            <p>
                <strong>Price</strong>
                <%=house.price%>
            </p>
            <%end%>
        </div>
        <!-- panel for settings -->
        <div class="tab-pane fade" role="tabpanel" id="settings">
            <p>.hnrth..</p>
            <%= link_to 'edit',  edit_user_registration_path, class:"btn btn-primary"%>
        </div>

    </div>
</div>
