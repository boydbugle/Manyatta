<!-- show the house tab if the current user owns any houses -->
<div class="tab-pane fade" role="tabpanel" id="houses">
  <%current_user.houses.each do |house|%>
  <p>
    <strong>Name:</strong>
    <%=house.name%>
  </p>
  <p>
    <strong>Location:</strong>
    <%=house.street%>
  </p>
  <p>
    <strong>Price</strong>
    <%=house.price%>
  </p>
  <%end%>
</div>
