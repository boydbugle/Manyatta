<div class="tab-pane" role="tabpanel" id="houses">
    <% if houses.any? %>
      <%@houses.each do |house|%>
      <%= image_tag%>
      <p>
        <strong>Name:</strong>
        <%=house.name%>
      </p>
      <p>
        <strong>Location:</strong>
        <%=house.street%>
      </p>
      <p>
        <strong>Price</strong>
        <%=house.price%>
      </p>
      <%end%>
    <% end %>
</div>
